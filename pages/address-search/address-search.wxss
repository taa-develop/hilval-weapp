.address-search {
  display: flex;
  flex-direction: column;
  height: 100vh;
}
.hotel-list {
  flex: 1;
  background-color: #fff;
  overflow-y: scroll;
}
.list-wrap {
  background-color: #fff;
  padding: 0 10px 10px 5px;
}
.tag-link {
  display: inline-block;
  margin-left: 5px;
  margin-bottom: 5px;
  padding: 5px 10px;
  border-radius: 5px;
  background-color: #efefef;
  color: #757575;
  font-size: 12px;
}
