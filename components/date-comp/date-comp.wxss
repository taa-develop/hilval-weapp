.title {
  margin: 20px 0;
  text-align: center;
  font-size: 16px;
}
.wrap {
  display: inline-block;
  width: 14.28%;
}
.date-item {
  width: 60rpx;
  height: 60rpx;
  margin: 20rpx auto;
  text-align: center;
  line-height: 60rpx;
}
.date-item.hide {
  visibility: hidden;
}
.date-item.week {
  background-color: transparent;
}
.date-item.transit {
  background-color: rgba(68, 146, 132, 0.2);
  border-radius: 50%;
}
.date-item.disable {
  opacity: 0.25;
}
.date-item.check-in {
  background-color: rgba(68, 146, 132, 0.8);
  border-radius: 50%;
}
.date-item.check-out {
  background-color: rgba(68, 146, 132, 0.8);
  border-radius: 50%;
}
